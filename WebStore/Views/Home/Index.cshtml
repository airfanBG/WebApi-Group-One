@{
    ViewData["Title"] = "Home Page";
}



@section Scripts{ 
    <script>
        $("#products").click(function () {
            $.ajax({
                url: '@Url.Action("Index", "Product")',
                method: "GET",               
                success: function (data) {
                    $("#main").html(data);
                }
            });
        });
    </script>
}